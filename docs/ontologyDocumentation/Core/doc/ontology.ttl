#################################################################
#    Classes
#################################################################

###  http://idea.rpi.edu/heals/ont/whattomake/core/BreakfastRecipe
:BreakfastRecipe rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :Recipe
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :isRecommendedForMeal ;
                                                              owl:hasValue :Breakfast
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 <http://purl.org/dc/terms/source> "\"Breakfast,\" Merriam-Webster. [Online]. Available: https://www.merriam-webster.com/dictionary/breakfast. [Accessed: 25-Oct-2018]." ;
                 rdfs:label "breakfast recipe" ;
                 <http://www.w3.org/2004/02/skos/core#definition> "a class that contains recipes of type breakfast" ;
                 <http://www.w3.org/2004/02/skos/core#scopeNote> "recipe class" .


###  http://idea.rpi.edu/heals/ont/whattomake/core/Characteristic
:Characteristic rdf:type owl:Class ;
                <http://purl.org/dc/terms/source> "\"Characteristic,\" Merriam-Webster. [Online]. Available: https://www.merriam-webster.com/dictionary/characterisitc. [Accessed: 25-Oct-2018]." ;
                rdfs:label "characteristic" ;
                <http://www.w3.org/2004/02/skos/core#definition> "a distinguishing trait, quality, or property" ;
                <http://www.w3.org/2004/02/skos/core#example> "Classes that inherit from this class represent different possible characteristics of food. Examples in this ontology include \"flavor\" and \"texture\" but these can be expanded as desired. Specific flavors and textures are represented as individuals that inherent from their respective subclass." ;
                <http://www.w3.org/2004/02/skos/core#scopeNote> "top-level class" .


###  http://idea.rpi.edu/heals/ont/whattomake/core/ChemicalFoodProduct
:ChemicalFoodProduct rdf:type owl:Class ;
                     rdfs:subClassOf :Food ;
                     <http://purl.org/dc/terms/source> "BioPortal, \"FoodOn,\"  November 2018. [Online]. Available: https://bioportal.bioontology.org/ontologies/FOODON/?p=classes&conceptid=http%3A%2F%2Fpurl.obolibrary.org%2Fobo%2FFOODON_03411041. [Accessed Nov. 19, 2018]." ;
                     rdfs:label "chemical food product" ;
                     <http://www.w3.org/2004/02/skos/core#definition> "a food substance derived from a nonliving source (e.g., salt, water or synthesized compounds)" ;
                     <http://www.w3.org/2004/02/skos/core#scopeNote> "used for ingredient classification" .


###  http://idea.rpi.edu/heals/ont/whattomake/core/Course
:Course rdf:type owl:Class ;
        <http://purl.org/dc/terms/source> "Wikipedia , \"Course (food).\" [Online]. Available: http://www.montignac.com/en/the-glycemic-index-concept/. [Accessed Nov. 1 2018]" ;
        rdfs:label "course" ;
        <http://www.w3.org/2004/02/skos/core#definition> "dfferent types of specific sets of food items that are served together during a meal" ;
        <http://www.w3.org/2004/02/skos/core#example> "The individuals that inhreit from this class represent courses of a meal. The recipe \"banana bread\" can be \"recommended for course\" \"dessert\". Additional courses can be added as individuals that inheret from this class." ;
        <http://www.w3.org/2004/02/skos/core#scopeNote> "top-level class" .


###  http://idea.rpi.edu/heals/ont/whattomake/core/DinnerRecipe
:DinnerRecipe rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :Recipe
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :isRecommendedForMeal ;
                                                           owl:hasValue :Dinner
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              <http://purl.org/dc/terms/source> "\"Dinner,\" Merriam-Webster. [Online]. Available: https://www.merriam-webster.com/dictionary/dinner. [Accessed: 25-Oct-2018]." ;
              rdfs:label "dinner recipe" ;
              <http://www.w3.org/2004/02/skos/core#definition> "a class that contains recipes of type dinner" ;
              <http://www.w3.org/2004/02/skos/core#scopeNote> "recipe class" .


###  http://idea.rpi.edu/heals/ont/whattomake/core/Flavor
:Flavor rdf:type owl:Class ;
        rdfs:subClassOf :Characteristic ;
        owl:disjointWith :Texture ;
        <http://purl.org/dc/terms/source> "\"Flavor,\" Merriam-Webster. [Online]. Available: https://www.merriam-webster.com/dictionary/flavor. [Accessed: 12-Oct-2018]." ;
        rdfs:label "flavor" ;
        <http://www.w3.org/2004/02/skos/core#definition> "the quality of something that affects the sense of taste" ;
        <http://www.w3.org/2004/02/skos/core#scopeNote> "food property" .


###  http://idea.rpi.edu/heals/ont/whattomake/core/Food
:Food rdf:type owl:Class ;
      <http://purl.org/dc/terms/source> "BBC, \"Food Ontology,\" ONTOLOGIES, March 2014. [Online]. Available: https://www.bbc.co.uk/ontologies/fo. [Accessed Sep. 29, 2018]." ;
      rdfs:label "food" ;
      <http://www.w3.org/2004/02/skos/core#definition> "something that can be eaten" ;
      <http://www.w3.org/2004/02/skos/core#example> "This class is the top-level element that represents different classifications of food. An ingredient individual will have the type of the most specific category it falls under. For example, \"chicken egg\" will be a type of \"egg\" which is a subclass of \"poultry product\" which is a subclass of \"primary food commodity of animal origin\" which is a subclass of \"food\". Additional classifications can be added at any level of the classification." ;
      <http://www.w3.org/2004/02/skos/core#scopeNote> "top-level class" .


###  http://idea.rpi.edu/heals/ont/whattomake/core/Ingredient
:Ingredient rdf:type owl:Class ;
            <http://purl.org/dc/terms/source> "Cambridge Dictionary, \"ingredient.\" [Online]. Available: https://dictionary.cambridge.org/us/dictionary/english/ingredient. [Accessed Nov. 10, 2018]" ;
            rdfs:label "ingredient" ;
            <http://www.w3.org/2004/02/skos/core#definition> "a food that is used with other foods in the preparation of a particular dish" ;
            <http://www.w3.org/2004/02/skos/core#example> "Individuals that are subclasses of this class represent ingredients that are used in a specific recipe. For example, the individual \"banana\" is a subclass of \"ingredient\" and the recipe \"banana bread\" \"has ingredient\" \"banana\". Ingredients can have the properties \"has glycymic index\" and \"has gluten\"." ;
            <http://www.w3.org/2004/02/skos/core#scopeNote> "top-level class" .


###  http://idea.rpi.edu/heals/ont/whattomake/core/LessThanOneHourRecipe
:LessThanOneHourRecipe rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :Recipe
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :hasCookTime ;
                                                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                         owl:onDatatype xsd:integer ;
                                                                                         owl:withRestrictions ( [ xsd:maxInclusive 60
                                                                                                                ]
                                                                                                              )
                                                                                       ]
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       <http://purl.org/dc/terms/source> "Dictionary.com, \"hour.\" [Online]. Available: https://www.dictionary.com/browse/hour. [Accessed Oct. 5, 2018]" ;
                       rdfs:label "less than one hour recipe" ;
                       <http://www.w3.org/2004/02/skos/core#definition> "a class that contains all recipes that only take one hour to cook" ;
                       <http://www.w3.org/2004/02/skos/core#scopeNote> "recipe class" .


###  http://idea.rpi.edu/heals/ont/whattomake/core/LunchRecipe
:LunchRecipe rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Recipe
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :isRecommendedForMeal ;
                                                          owl:hasValue :Lunch
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             <http://purl.org/dc/terms/source> "\"Lunch,\" Merriam-Webster. [Online]. Available: https://www.merriam-webster.com/dictionary/lunch. [Accessed: 25-Oct-2018]." ;
             rdfs:label "lunch recipe" ;
             <http://www.w3.org/2004/02/skos/core#definition> "a class that contains recipes of type lunch" ;
             <http://www.w3.org/2004/02/skos/core#scopeNote> "recipe class" .


###  http://idea.rpi.edu/heals/ont/whattomake/core/Meal
:Meal rdf:type owl:Class ;
      <http://purl.org/dc/terms/source> "Wikipedia, \"Meal.\" [Online]. Available: https://en.wikipedia.org/wiki/Meal. [Accessed: Oct. 25, 2018]" ;
      rdfs:label "meal" ;
      <http://www.w3.org/2004/02/skos/core#definition> "a class that links a recipe to a certain meal" ;
      <http://www.w3.org/2004/02/skos/core#example> "The individuals that inhreit from this class represent different types of meals. The recipe \"kamut pancakes\" can be \"recommended for meal\" \"breakfast\". Additional meals can be added as individuals that inheret from this class." ;
      <http://www.w3.org/2004/02/skos/core#scopeNote> "top-level class" .


###  http://idea.rpi.edu/heals/ont/whattomake/core/Measurement
:Measurement rdf:type owl:Class ;
             <http://purl.org/dc/terms/source> "\"Measure,\" Merriam-Webster. [Online]. Available: https://www.merriam-webster.com/dictionary/measure. [Accessed: 12-Oct-2018]." ;
             rdfs:label "measurement" ;
             <http://www.w3.org/2004/02/skos/core#definition> "an adequate or due portion" ;
             <http://www.w3.org/2004/02/skos/core#example> "This class contains subclasses that represent different kinds of measurements relating to cooking. For example \"time measurement\" is a class that contains the individuals \"minute\" and \"hour\" and is used to represent the amount of time a recipe takes to make or cook." ;
             <http://www.w3.org/2004/02/skos/core#scopeNote> "top-level class" .


###  http://idea.rpi.edu/heals/ont/whattomake/core/MeatFromChicken
:MeatFromChicken rdf:type owl:Class ;
